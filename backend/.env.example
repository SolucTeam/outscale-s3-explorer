
# Application Configuration
NODE_ENV=development
PORT=5000
JWT_SECRET=your-super-secret-jwt-key-change-this-in-production
JWT_EXPIRES_IN=24h

# CORS Configuration
FRONTEND_URL=http://localhost:5173

# Rate Limiting
RATE_LIMIT_WINDOW_MS=900000
RATE_LIMIT_MAX_REQUESTS=100

# Logging
LOG_LEVEL=info
LOG_FILE=logs/app.log

# AWS/Outscale Configuration
AWS_REGION=eu-west-2
S3_ENDPOINT_EU_WEST_2=https://oos.eu-west-2.outscale.com
S3_ENDPOINT_US_EAST_2=https://oos.us-east-2.outscale.com
S3_ENDPOINT_US_WEST_1=https://oos.us-west-1.outscale.com
S3_ENDPOINT_CLOUDGOUV_EU_WEST_1=https://oos.cloudgouv-eu-west-1.outscale.com
S3_ENDPOINT_AP_NORTHEAST_1=https://oos.ap-northeast-1.outscale.com

# Security
BCRYPT_ROUNDS=12
SESSION_TIMEOUT=86400000
